{
    "name": "etiam",
    "version": "1.0.0",
    "description": "[![Gitpod ready-to-code](https://img.shields.io/badge/Gitpod-ready--to--code-blue?logo=gitpod)](https://gitpod.io/#https://github.com/robertocdn/Etiam)",
    "main": "dist/index.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "npm run build-ts && npm run lint",
        "build-ts": "tsc",
        "start": "nodemon src/index.ts",
        "serve": "node dist/index.js",
        "serve-debug": "nodemon --inspect dist/index.js",
        "debug": "npm run build && npm run watch-debug",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "watch-ts": "tsc -w"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/robertocdn/Etiam.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/robertocdn/Etiam/issues"
    },
    "homepage": "https://github.com/robertocdn/Etiam#readme",
    "devDependencies": {
        "@types/express": "^4.17.7",
        "@types/node": "^14.0.26",
        "@types/eslint": "^6.1.1",
        "@types/jest": "^24.0.23",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^2.3.1",
        "@typescript-eslint/parser": "^2.3.1",
        "jest": "^29.5.0",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.4.1",
        "eslint": "^7.5.0",
        "nodemon": "^2.0.4",
        "typescript": "^3.9.7"
    },
    "dependencies": {
        "compression": "^1.7.4",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-validator": "^6.6.0"
    }
}
